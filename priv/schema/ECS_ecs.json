{
	"$schema": "http://json-schema.org/draft-07/schema#",
	"$id": "http://sigscale.org/schema/cgf/ECS_ecs.json",
	"title": "Elastic Common Schema (ECS) ECS",
	"$defs": {
		"ECS_ecs": {
			"$id": "#/$defs/ECS_ecs",
			"description": "Meta-information specific to ECS.",
			"type": "object",
			"properties": {
				"ecs": {
					"type": "object",
					"properties": {
						"version": {
							"description": "ECS version this event conforms to.",
							"type": "string",
							"enum": [
								"8.5",
								"8.6"
							]
						}
					},
					"required": [
						"version"
					]
				}
			},
			"required": [
				"ecs"
			]
		}
	}
}
